{
	"paths": {
		"src": "resources/_assets",
		"dest": "resources/assets/compiled",
		"compiled": "compiled"
	},
	"config": {
		"browserSync": {
			"files": [],
			"ghostMode": false,
			"notify": false,
			"open": false,
			"port": 5757,
			"proxy": "baseplate.dev.com:80",
			"snippetOptions": {
				"rule": {
					"match": "</body>"
				}
			},
			"ui": false,
			"sync": false
		},
		"css": {
			"autoprefixer": {
				"cascade": false
			},
			"cleanCss": {},
			"purgeCssWhitelist": [],
			"purgeCssWhitelistPatterns": []
		},
		"js": {
			"lint": true
		}
	}
}
